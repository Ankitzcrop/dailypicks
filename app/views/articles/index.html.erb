  <body>
    <div id="customCarousel" class="carousel slide" data-bs-ride="carousel">
    <!-- Indicators -->
      <div class="carousel-indicators">
        <button type="button" data-bs-target="#customCarousel" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
        <button type="button" data-bs-target="#customCarousel" data-bs-slide-to="1" aria-label="Slide 2"></button>
        <button type="button" data-bs-target="#customCarousel" data-bs-slide-to="2" aria-label="Slide 3"></button>
      </div>

      <!-- Carousel Items -->
      <div class="carousel-inner">
        <!-- Slide 1 -->
        <div class="carousel-item active" style="background-image: url('<%= asset_path('image_3.png') %>'); background-size: cover; background-position: center; height: 700px;">
          <div class="container h-100 d-flex flex-column justify-content-start align-items-start text-dark">
            <h2 class="fw-bold mt-5 text-muted" style="font-size: 5rem; line-height: 1.7;">Fresh Fruits & <br> Vegetable</h2>
            <div class="d-flex justify-content-center mt-5">
              <a href="#" class="btn btn-lg rounded-pill px-5 py-2 w-100 w-sm-75 w-md-75 w-lg-75" 
                 style="background-color: #88bb2f; color: white; max-width: 300px;">
                 Now on Daily Picks
              </a>
            </div>
          </div>
        </div>

        <!-- Slide 2 -->
        <div class="carousel-item" style="background-image: url('<%= asset_path('image_3.png') %>'); background-size: cover; background-position: center; height: 700px;">
          <div class="container h-100 d-flex flex-column justify-content-start align-items-start">
            <h2 class="fw-bold mt-5 text-muted" style="font-size: 5rem; line-height: 1.7;">Fresh Fruits & <br> Vegetable</h2>
            <div class="d-flex justify-content-center mt-5">
              <a href="#" class="btn btn-lg rounded-pill px-5 py-2 w-100 w-sm-75 w-md-75 w-lg-75" 
                 style="background-color: #88bb2f; color: white; max-width: 300px;">
                 Now on Daily Picks
              </a>
            </div>
          </div>
        </div>

        <!-- Slide 3 -->
        <div class="carousel-item" style="background-image: url('<%= asset_path('image_3.png') %>'); background-size: cover; background-position: center; height: 700px;">
          <div class="container h-100 d-flex flex-column justify-content-start align-items-start">
            <h2 class="fw-bold mt-5 text-muted" style="font-size: 5rem; line-height: 1.7;">Fresh Fruits & <br> Vegetable</h2>
            <div class="d-flex justify-content-center mt-5">
              <a href="#" class="btn btn-lg rounded-pill px-5 py-2 w-100 w-sm-75 w-md-75 w-lg-75 button"style="max-width: 300px;">Now on Daily Picks</a>
            </div>
          </div>
        </div>
      </div>

      <!-- Controls -->
      <button class="carousel-control-prev" type="button" data-bs-target="#customCarousel" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button class="carousel-control-next" type="button" data-bs-target="#customCarousel" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>

